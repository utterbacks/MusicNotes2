<%- include ("../partials/header") %>


<form class="ui form" action="/users/<%= currentUser._id %>/findStudents" method="POST">
    <div class="field">
      <select class="ui dropdown" name="studentName">
        <option value="">Students</option>
        <% for(var i = 0; i < students.length; i++){%>
        <option value="0" name="studentName" type="text"><%= students[i].firstName %></option>
        <% } %>
      </select> 
    </div>
    <div class="field">
        <input type="submit" value="Add Student">
    </div>
</form>

